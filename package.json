{
  "name": "backend-book-ts",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build .",
    "start": "yarn build && NODE_ENV=production node dist/index.js",
    "start:dev": "export DEBUG=app:* && nodemon",
    "prepare": "husky install",
    "test": "NODE_ENV=test jest --coverage --silent --runInBand --detectOpenHandles",
    "seed:user": "export DEBUG=* && ts-node src/utils/seeds/user.seed.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "mongoose": "^6.8.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "swagger-jsdoc": "^6.2.7",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": ">=7",
    "jest": "^29.3.1",
    "lint-staged": ">=10",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
